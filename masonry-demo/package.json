      "grunt-contrib-imagemin": "~0.8.0",
      "grunt-newer": "~0.7.0",
      "grunt-bower-task": "~0.4.0",
      "grunt-browserify": "~3.0.0",
      "grunt-pagespeed": "~0.3.0",
      "grunt-concurrent": "~1.0.0",
      "browserify": "~5.11.1",
      "browserify-shim": "~3.7.0",
      "ngrok": "~0.1.97"
  },
