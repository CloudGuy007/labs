{
  "name": "site-start",
  "version": "1.0.0",
  "author": "Staplegun.US",
  "descripton": "A Boilerplate starter-kit for starting development right - the Staplegun way. Includes build automation and common CSS and JS libraries",
  "homepage": "https://github.com/Staplegun-US/site-start",
  "repository" : {
    "type"  : "git",
    "url"   : "https://github.com/Staplegun-US/site-start"
  },
  "bugs": {
    "url": "https://github.com/Staplegun-US/site-start/issues"
  },
  "browser": {
    "jquery": "./vendor/bower_components/jquery/jquery.js",
    "underscore": "./vendor/bower_components/underscore/underscore.js",
    "modernizr": "./vendor/bower_components/modernizr/modernizr.js"
  },
  "browserify-shim": {
